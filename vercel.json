{
  "routes": [{ "src": "/(.*)", "dest": "/home.html" }],
  "builds": [
    { "src": "home.html", "use": "@vercel/static" },
    { "src": "css/(.*)", "use": "@vercel/static" },
    { "src": "js/(.*)", "use": "@vercel/static" },
    { "src": "images/(.*)", "use": "@vercel/static" }
  ],
  "outputDirectory": "Educational-Website"
}
